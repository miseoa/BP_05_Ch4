import turtle
t=turtle.Turtle()
t.shape("turtle")

color1=input("색상 #1을 입력하시오.:")
색상 #1을 입력하시오.:pink
color2=input("색상 #2를 입력하시오.:")
색상 #2를 입력하시오.:blue
color3=input("색상 #3을 입력하시오.:")
색상 #3을 입력하시오.:green

t.fillcolor(color1)
t.begin_fill()
t.circle(50)
t.end_fill()

t.up()
t.fd(100)
t.down()

t.fillcolor(color2)
t.begin_fill()
t.circle(50)
t.end_fill()

t.up()
t.fd(100)
t.down()

t.fillcolor(color3)
t.begin_fill()
t.circle(50)
t.end_fill()
